 sort: POPULARITY_DESC\",\r\n      allTime: \"sort: POPULARITY_DESC\",\r\n    };\r\n    var query =\r\n      `query {\r\n  Page(page: 1, perPage: 100) {\r\n    media(type: ANIME, ` +\r\n      mapping[sectionId] +\r\n      `) {\r\n      id\r\n      title {\r\n        romaji\r\n      }\r\n      status\r\n      description\r\n      coverImage {\r\n        extraLarge\r\n      }\r\n      season\r\n      seasonYear\r\n      trending\r\n    }\r\n  }\r\n}`;\r\n    var url = \"https://graphql.anilist.co\",\r\n      options = {\r\n        method: \"POST\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Accept: \"application/json\",\r\n        },\r\n        body: JSON.stringify({\r\n          query: query,\r\n        }),\r\n      };\r\n    fetch(url, options)\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        this.list = data.data.Page.media;\r\n        console.log(this.list);\r\n      });\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#search {\r\n  padding: 100px 188px;\r\n  background-color: rgb(229, 235, 241);\r\n}\r\n.search__container {\r\n  max-width: 1200px;\r\n  width: 100%;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}