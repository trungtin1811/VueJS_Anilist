{"ast":null,"code":"import axios from \"axios\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"data-sub-section\",\n  props: {\n    no: Number\n  },\n\n  data() {\n    return {};\n  },\n\n  methods: {},\n\n  setup() {\n    var query = `query{\n    Page (page: 1, perPage: 5) {\n      pageInfo {\n        total\n        currentPage\n        lastPage\n        hasNextPage\n        perPage\n\n      }\n      media (type:ANIME) {\n        id\n        title {\n          romaji\n        }\n        status\n        description\n      }\n    }\n  }`;\n    var url = \"https://graphql.anilist.co\",\n        options = {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n        Accept: \"application/json\"\n      },\n      body: JSON.stringify({\n        query: query\n      })\n    };\n    const animeList = ref([]);\n\n    const getAll = async () => {\n      try {\n        const res = await axios.get(url, options);\n        animeList.value = res.data;\n      } catch {}\n    };\n\n    function handleResponse(response) {\n      return response.json().then(function (json) {\n        return response.ok ? json : Promise.reject(json);\n      });\n    }\n\n    function handleData(data) {\n      animeList.value = ref(data.data.Page.media);\n      console.log(animeList);\n    }\n\n    fetch(url, options).then(handleResponse).then(handleData);\n    return {\n      animeList\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,eAAe;AACbC,MAAI,EAAE,kBADO;AAEbC,OAAK,EAAE;AACLC,MAAE,EAAEC;AADC,GAFM;;AAKbC,MAAI,GAAG;AACL,WAAO,EAAP;AACD,GAPY;;AAQbC,SAAO,EAAE,EARI;;AASbC,OAAK,GAAG;AACN,QAAIC,KAAI,GAAK;;;;;;;;;;;;;;;;;;;IAAb;AAoBA,QAAIC,GAAE,GAAI,4BAAV;AAAA,QACEC,OAAM,GAAI;AACRC,YAAM,EAAE,MADA;AAERC,aAAO,EAAE;AACP,wBAAgB,kBADT;AAEPC,cAAM,EAAE;AAFD,OAFD;AAMRC,UAAI,EAAEC,IAAI,CAACC,SAAL,CAAe;AACnBR,aAAK,EAAEA;AADY,OAAf;AANE,KADZ;AAYA,UAAMS,SAAQ,GAAIjB,GAAG,CAAC,EAAD,CAArB;;AACA,UAAMkB,MAAK