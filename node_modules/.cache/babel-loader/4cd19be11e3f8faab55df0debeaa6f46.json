nding.co\",\r\n\r\n    function getOptions(query) {\r\n      return {\r\n        method: \"POST\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Accept: \"application/json\",\r\n        },\r\n        body: JSON.stringify({\r\n          query: query,\r\n        }),\r\n      };\r\n    }\r\n    const listTrending = ref();\r\n    fetch(url, getOptions(queryThisSeason))\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        listTrending.value = data.data.Page.media;\r\n      });\r\n    const listThisSeason = ref();\r\n    fetch(url, getOptions(queryTrending))\r\n      .then((response) => response.json())\r\n      .then((data) => {\r\n        listThisSeason.value = data.data.Page.media;\r\n      });\r\n    return { listTrending, listThisSeason };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#content {\r\n  padding: 100px 188px;\r\n  background-color: rgb(229, 235, 241);\r\n}\r\n.section-title {\r\n  font-weight: 700 !important;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}